alias desk='cd ~/Desktop'
alias cd..='cd ..'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias ~='cd ~'
alias s='ls -la'
alias T='tree'
alias t='tree -L 2'
alias gr='grep -i'
alias cw='date +%V'
alias json-formatter='python -m json.tool'
alias xml-formatter='xmllint --format -'
alias cppwd='echo -n "$(pwd)" | pbcopy'
